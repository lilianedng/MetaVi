{
	"Serilog": {
		"MinimumLevel": {
			"Default": "Information",
			"Override": {
				"Microsoft.AspNetCore": "Warning",
				"OpenTelemetry": "Warning"
			}
		}
	},
	"Logstash": {
		"Uri": "http://localhost:9250"
	},
	"ConnectionStrings": {
		"MongoDb": "mongodb://localhost:27017",
		"Redis": "localhost:6379"
	},
	"JaegerExporterOptions": {
		"AgentHost": "localhost"
	},
	"Initialization": {
		"MigrateDatabase": true,
		"CreateIndices": true
	},
	"RabbitMQConfiguration": {
		"HostName": "localhost",
		"UserName": "user",
		"Password": "password"
	},
	"CachingConfigurations": {
		"VideoCacheDurationInSeconds": 600,
		"UserProfileCacheDurationInSeconds": 600
	},
	"ElasticsearchConfiguration": {
		"Uri": "http://localhost:9200"
	},
	"StorageConfiguration": {
		"BaseUri": "http://localhost:14200"
	},
	"Urls": {
		"IdentityProvider": "http://localhost:15100"
	},
	"MetricsSyncConfiguration": {
		"SyncDelaySeconds": 1,
		"MaxConcurrentProcessingtLimit": 4,
		"MaxProcessingRateLimit": 5,
		"FetchCount": 200
	}
}